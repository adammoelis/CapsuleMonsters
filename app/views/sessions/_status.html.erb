<% if user_signed_in? %>
  <%= current_user.username %>
  <%= link_to "Log Out", logout_path, method: 'delete', id: 'log-out-button' %>
<% else %>
  <%= link_to "Sign Up!", new_user_path, remote: true, id: 'sign-up-button' %>
  <%= link_to "Log In", login_path, remote: true, id: 'log-in-button' %>
<% end %>
